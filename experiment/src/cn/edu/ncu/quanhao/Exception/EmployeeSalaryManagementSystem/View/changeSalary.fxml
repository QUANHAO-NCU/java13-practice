<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="319.0" prefWidth="275.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal"
            fx:controller="cn.edu.ncu.quanhao.Exception.EmployeeSalaryManagementSystem.Controller.changeSalary">
    <children>
        <Label fx:id="id" layoutX="-1.0" layoutY="-1.0" prefHeight="53.0" prefWidth="137.0" text="工号">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label fx:id="name" layoutX="138.0" prefHeight="53.0" prefWidth="137.0" text="姓名">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label fx:id="position" layoutY="52.0" prefHeight="62.0" prefWidth="276.0" text="职位">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label fx:id="currentSalary" layoutX="3.0" layoutY="114.0" prefHeight="104.0" prefWidth="129.0" text="当前工资">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <TextField fx:id="newSalary" layoutX="134.0" layoutY="114.0" prefHeight="104.0" prefWidth="145.0"
                   promptText="输入新的工资"/>
        <Button fx:id="confirm" layoutX="67.0" layoutY="238.0" mnemonicParsing="false" onAction="#change"
                prefHeight="34.0" prefWidth="122.0" text="确认修改"/>
    </children>
</AnchorPane>
